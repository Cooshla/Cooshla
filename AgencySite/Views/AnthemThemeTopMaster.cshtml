            <a class="remove-search"><i class="fa fa-times"></i></a>
        </div>
        <!-- planner window -->
        <div class="planner-modal grey custom-scroll">
            <!-- planner-scrollarea adds custom scrollbar for planner -->
            <div id="planner-scrollarea">
                <div class="content-block">
                    <div class="row">
                        <!-- planner heading -->
                        <div class="ten columns centered">
                            <h2 class="bold">PROJECT PLANNER</h2>
                            <p class="intro s-bold">
                                Anthem comes with a fully working project planner script. Easily catch leads and get forms mailed to your email account in a nice table layout.
                            </p>
                            <hr class="thin grey">
                        </div>
                    </div>
                    <div class="row">
                        <div class="ten columns centered smallpadding">
                            <form method="post" action="projectplanner.php" onsubmit="return CheckAll(this);" id="form1">
                                <!-- first part -->
                                <h5 class="bold purple-text">HELLO THERE</h5>
                                <div class="clear-20">
                                </div>
                                <div class="row">
                                    <div class="field six columns">
                                        <label for="name">Your Name*</label>
                                        <input class="input" type="text" id="name" name="fieldnm_1" placeholder="How do we address you...">
                                    </div>
                                    <div class="field six columns">
                                        <label for="email">Your E-mail*</label>
                                        <input class="input" type="email" id="email" name="fieldnm_2" placeholder="Where should we write to you...">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="field six columns">
                                        <label for="telephone">Telephone number</label>
                                        <input class="input" type="tel" id="telephone" name="fieldnm_3" placeholder="Can we give you a ring...">
                                    </div>
                                    <div class="field six columns">
                                        <label for="company">Company name</label>
                                        <input class="input" type="text" id="company" name="fieldnm_4" placeholder="Are you representing someone...">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="field six columns">
                                        <label for="website">Website URL i-e http://www.website.com</label>
                                        <input class="input" type="url" id="website" name="fieldnm_5" placeholder="Do you have a website...">
                                    </div>
                                    <div class="field six columns">
                                        <label for="location">Your location</label>
                                        <input class="input" type="text" id="location" name="fieldnm_6" placeholder="Where are you located...">
                                    </div>
                                </div>
                                <div class="clear-50">
                                </div>
                                <!-- second part -->
                                <h5 class="bold purple-text">PROJECT DETAILS</h5>
                                <div class="clear-20">
                                </div>
                                <div class="field row">
                                    <label for="message">Please describe your project or idea</label>
                                    <div class="textarea">
                                        <textarea class="input textarea" rows="4" id="message" name="fieldnm_7"></textarea>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="field six columns">
                                        <label for="req">Project requirements</label>
                                        <input class="input" type="text" id="req" name="fieldnm_8" placeholder="Website, Branding, Mobile App etc">
                                    </div>
                                    <div class="field six columns">
                                        <label for="start">When do you hope to start</label>
                                        <input class="input" type="text" id="start" name="fieldnm_9">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="field six columns">
                                        <label for="launch">When do you hope to launch</label>
                                        <input class="input" type="text" id="launch" name="fieldnm_10">
                                    </div>
                                    <div class="field six columns">
                                        <label for="budget">Your budget*</label>
                                        <input class="input" type="number" id="budget" name="fieldnm_11" placeholder="What are you willing to invest">
                                    </div>
                                </div>
                                <div class="clear-50">
                                </div>
                                <!-- third part -->
                                <h5 class="bold purple-text">DESIGN AND FEEL</h5>
                                <div class="clear-20">
                                </div>
                                <div class="field row">
                                    <label for="look">Please describe your desired look and feel</label>
                                    <div class="textarea">
                                        <textarea class="input textarea" rows="4" id="look" name="fieldnm_12" placeholder="Clean, Modern, Minimal etc"></textarea>
                                    </div>
                                </div>
                                <div class="field row">
                                    <label for="insp">Any inspiration for your project</label>
                                    <div class="textarea">
                                        <textarea class="input textarea" rows="4" id="insp" name="fieldnm_13" placeholder="Websites, Designs you like etc"></textarea>
                                    </div>
                                </div>
                                <div class="field row">
                                    <label for="extra">Anything else you want us to know</label>
                                    <div class="textarea">
                                        <textarea class="input textarea" rows="4" id="extra" name="fieldnm_14"></textarea>
                                    </div>
                                </div>
                                <!-- submit button -->
                                <input type="submit" name="Submit" value="Submit Form" class="purple submit">
                            </form>
                            <hr class="thin grey">
                            <div class="twelve columns alpha">
                                <p>
                                    <span class="purple-text bold">Privacy Policy:</span>&nbsp; We will never share your information with anyone. We will only contact you with regards to your enquiry.
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- close planner button -->
                <a class="remove-planner"><i class="fa fa-times"></i></a>
            </div>
            <!--planner scrollarea -->
        </div>
        <!-- planner -->
        <!--showreel modal -->
        <div class="video-modal" id="video-modal">
            <div class="youtube video">
                <!-- video source is added in ../js/main.js  under heading video modal -->
            </div>
            <a class="remove-video switch" gumby-trigger="|#video-modal"><i class="fa fa-times"></i></a>
        </div>

        <!-- back to top button displays after 850px
        vertical scroll value in ../js/main.js -->
        <div class="back-to-top">
            <a id="top-trigger">
                <i class="fa fa-angle-up"></i>
            </a>
        </div>
    </div>
